{
  "name": "matcha-client",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.2"
  },
  "scripts": {
    "build:css": "node-sass --include-path scss src/styles/index.scss src/styles/index.css && postcss src/styles/index.css -o src/index.css",
    "watch:css": "node-sass --include-path scss src/styles/index.scss src/styles/index.css && postcss src/styles/index.css -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^6.1.3",
    "postcss-nested": "^4.1.2",
    "postcss-scss": "^2.0.0",
    "styled-components": "^4.3.2",
    "tailwindcss": "^1.1.2"
  }
}
